PORT=5000
CLIENT_URL=http://localhost:4000
NODE_ENV=development

# Local MongoDB (default port)
MONGODB_URL=mongodb://127.0.0.1:27017

# JWT
ACCESS_TOKEN_SECRET=your_access_token_secret_here
